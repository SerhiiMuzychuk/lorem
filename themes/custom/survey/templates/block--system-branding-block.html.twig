{% extends "block.html.twig" %}

{% block content %}
  {% if site_logo %} 
    <div class="site-logo">
      <a href="{{ url('<front>') }}" title="{{ 'Home'|t }}" rel="home">
        <img src="{{ site_logo }}" alt="{{ 'Home'|t }}" />
      </a>
    </div>
  {% endif %}
  <div class="container">
  {% if site_name %}
    <div>
      <a href="{{ url('<front>') }}" title="{{ 'Home'|t }}" rel="home">
        <div class="site-name">{{ site_name }}</div>
      </a>
    </div>
  {% endif %}
  {% if site_slogan %}
    
    <div class="site-slogan">
      {{ site_slogan }}</div>
  </div>
  {% endif %}
{% endblock %}